{
    "name": "@fluxmedia/r2",
    "version": "0.1.0",
    "description": "Cloudflare R2 provider for FluxMedia",
    "keywords": [
        "fluxmedia",
        "r2",
        "cloudflare",
        "media",
        "upload",
        "cloud"
    ],
    "homepage": "https://fluxmedia.dev",
    "repository": {
        "type": "git",
        "url": "https://github.com/fluxmediajs/fluxmedia.git",
        "directory": "packages/r2"
    },
    "license": "MIT",
    "author": "FluxMedia Contributors",
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "!dist/**/*.map"
    ],
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf dist",
        "dev": "tsup --watch"
    },
    "dependencies": {
        "@fluxmedia/core": "workspace:*"
    },
    "devDependencies": {
        "@aws-sdk/client-s3": "^3.980.0",
        "@aws-sdk/lib-storage": "^3.980.0",
        "@types/node": "^22.19.4",
        "tsdown": "^0.20.1",
        "typescript": "^5.3.3"
    },
    "peerDependencies": {
        "@aws-sdk/client-s3": "^3.0.0",
        "@aws-sdk/lib-storage": "^3.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}