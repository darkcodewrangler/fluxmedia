{
    "name": "@fluxmedia/s3",
    "version": "0.1.0-alpha.0",
    "description": "AWS S3 provider for FluxMedia",
    "keywords": [
        "fluxmedia",
        "s3",
        "aws",
        "media",
        "upload",
        "cloud"
    ],
    "homepage": "https://fluxmedia.dev",
    "repository": {
        "type": "git",
        "url": "https://github.com/fluxmediajs/fluxmedia.git",
        "directory": "packages/s3"
    },
    "license": "MIT",
    "author": "FluxMedia Contributors",
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "!dist/**/*.map"
    ],
    "scripts": {
        "build": "tsdown",
        "clean": "rm -rf dist",
        "dev": "tsdown --watch"
    },
    "dependencies": {
        "@fluxmedia/core": "workspace:*"
    },
    "devDependencies": {
        "@aws-sdk/client-s3": "^3.980.0",
        "@aws-sdk/lib-storage": "^3.980.0",
        "@types/node": "^20.11.5",
        "tsdown": "^0.20.1",
        "typescript": "^5.3.3"
    },
    "peerDependencies": {
        "@aws-sdk/client-s3": "^3.0.0",
        "@aws-sdk/lib-storage": "^3.0.0",
        "@aws-sdk/s3-request-presigner": "^3.0.0"
    },
    "peerDependenciesMeta": {
        "@aws-sdk/s3-request-presigner": {
            "optional": true
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}